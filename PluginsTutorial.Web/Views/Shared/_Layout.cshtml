							<li><a href="#">One more separated link</a></li>
						</ul>
					</li>
				</ul>
			</div>
			<!--/.nav-collapse -->
		</div>
	</div>
	
	<div class="container">
		@RenderBody()
		<hr>
		<footer>
			<p>&copy; Company 2014</p>
		</footer>
